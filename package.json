{
  "name": "quadrats",
  "description": "Quadrats",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quadrats/quadrats.git"
  },
  "author": "Rytass",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run clean && lerna clean",
    "build:clean": "lerna run build:clean --stream --parallel",
    "build": "lerna run build --stream --sort",
    "format": "prettier './**/*.{json,md,mdx}' --write",
    "lint:css": "stylelint ./**/*.scss",
    "lint:ts": "eslint . --ext .js,.ts,.tsx,.mdx",
    "lint": "npm run lint:ts && npm run lint:css",
    "test:clean": "rm -rf ./coverage",
    "test:coverage": "jest --coverage",
    "test": "jest",
    "storybook:start": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:deploy": "storybook-to-ghpages --out ./public",
    "commit": "git-cz",
    "release": "lerna version patch && npm run build && lerna publish from-package",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/cli": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-webpack5": "^7.0.18",
    "@storybook/storybook-deployer": "^2.8.16",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "awesome-typescript-loader": "^5.2.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.6",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lerna": "^6.6.2",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.23.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "slate-hyperscript": "^0.77.0",
    "storybook": "^7.0.18",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.0.4",
    "webpack": "^5.84.1"
  },
  "dependencies": {}
}
