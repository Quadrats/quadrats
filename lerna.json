{
  "packages": ["packages/*"],
  "npmClient": "yarn",
  "npmClientArgs": ["--no-lockfile"],
  "version": "0.7.1",
  "command": {
    "version": {
      "ignoreChanges": ["**/__fixtures__/**", "**/__mocks__/**", "**/__tests__/**", "*.spec.*", "**/*.md"],
      "message": "chore(release): %s",
      "conventionalCommits": true,
      "yes": true
    },
    "publish": {
      "contents": "dist"
    }
  },
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
